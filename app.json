{
  "expo": {
    "name": "MyDeSoMobile",
    "slug": "mydesomobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon-1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#156BFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.desomobile",
      "icon": "./assets/icons/app-icon-1024.png"
    },
    "android": {
      "package": "com.yourname.desomobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "desomobile"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-foreground-1024.png",
        "backgroundColor": "#0B69FF"
      },
      "notification": {
        "icon": "./assets/icons/mono-icon-1024.png",
        "color": "#0B69FF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser"
    ],
    "scheme": "desomobile",
    "platforms": [
      "android",
      "ios"
    ],
    "extra": {
      "eas": {
        "projectId": "d00e02ab-dfb9-44e8-a5e7-3d0f94138b26"
      }
    }
  }
}
